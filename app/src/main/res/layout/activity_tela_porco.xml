<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="butecogaragem.cursoandroid.com.butecogaragem.TelaPorco"
    android:background="#383333">

    <Button
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:text="COSTELINHA"
        android:id="@+id/button35"
        android:layout_marginTop="59dp"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/button36"
        android:layout_alignStart="@+id/button36"
        android:backgroundTint="#86ffffff"
        android:textColor="#000000"
        android:textSize="@dimen/texto_costelinha"
        android:textStyle="bold" />

    <Button
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:text="PERNIL"
        android:id="@+id/button36"
        android:layout_below="@+id/button35"
        android:layout_centerHorizontal="true"
        android:backgroundTint="#86ffffff"
        android:textColor="#000000"
        android:textSize="@dimen/texto_pernil"
        android:textStyle="bold" />

    <ImageView
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:id="@+id/imageView15"
        android:src="@drawable/c"
        android:layout_below="@+id/button36"
        android:layout_alignLeft="@+id/button36"
        android:layout_alignStart="@+id/button36" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageView16"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/abc_vector_test" />
</RelativeLayout>
