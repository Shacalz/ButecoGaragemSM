<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="butecogaragem.cursoandroid.com.butecogaragem.TelaPeixe"
    android:background="#383333">

    <Button
        android:layout_width="175dp"
        android:layout_height="100dp"
        android:text="PALITO DE TILAPIA"
        android:id="@+id/button39"
        android:backgroundTint="#86ffffff"
        android:textColor="#000000"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="@dimen/texto_palito_tilapia"
        android:textStyle="bold" />

    <Button
        android:layout_width="175dp"
        android:layout_height="100dp"
        android:text="PEDAÇOS DE TILAPIA"
        android:id="@+id/button40"
        android:backgroundTint="#86ffffff"
        android:textColor="#000000"
        android:layout_above="@+id/button42"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:textSize="@dimen/texto_pedaço_tilapia"
        android:textStyle="bold" />

    <Button
        android:layout_width="175dp"
        android:layout_height="100dp"
        android:text="BOLINHO DE PEIXE"
        android:id="@+id/button41"
        android:backgroundTint="#86ffffff"
        android:textColor="#000000"
        android:layout_below="@+id/button39"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="@dimen/texto_bolinho_peixe"
        android:textStyle="bold" />

    <Button
        android:layout_width="175dp"
        android:layout_height="100dp"
        android:text="BACALHAU"
        android:id="@+id/button42"
        android:layout_alignBottom="@+id/button41"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:backgroundTint="#86ffffff"
        android:textColor="#000000"
        android:textSize="@dimen/texto_bacalhau"
        android:textStyle="bold" />

    <Button
        android:layout_width="175dp"
        android:layout_height="100dp"
        android:text="MEDALHAO PROVOLONE"
        android:id="@+id/button43"
        android:backgroundTint="#86ffffff"
        android:textColor="#000000"
        android:layout_alignTop="@+id/button44"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="@dimen/texto_medalhao_provolone"
        android:textStyle="bold" />

    <Button
        android:layout_width="175dp"
        android:layout_height="100dp"
        android:text="MEDALHÃO BACON"
        android:id="@+id/button44"
        android:backgroundTint="#86ffffff"
        android:textColor="#000000"
        android:layout_below="@+id/button42"
        android:layout_alignLeft="@+id/button42"
        android:layout_alignStart="@+id/button42"
        android:textSize="@dimen/texto_medalhao_bacon"
        android:textStyle="bold" />

    <ImageView
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:id="@+id/imageView19"
        android:layout_below="@+id/button43"
        android:layout_centerHorizontal="true"
        android:src="@drawable/b" />

    <ImageButton
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton5"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/abc_vector_test" />
</RelativeLayout>
